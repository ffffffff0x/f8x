# -p
Proxy(){

    echo -e "\033[1;33m\n>> Do you want to update DNS settings? [Y/n,Default Y]\033[0m"
    Change_DNS_IP
    Mirror

    if [ $Docker_OK ==  ] 2>> /tmp/f8x_error.log
    then
        echo -e "\033[1;33m\n>> Installing resolvconf\n\033[0m"
        DNS_T00ls
        echo -e "\033[1;33m\n>> Configuring the base compilation environment\n\033[0m"
        Proxychains_Install
    fi

    echo -e "\033[1;33m\n>> Configuring the pip proxy\n\033[0m"
    pip_Proxy > /dev/null

    Echo_INFOR "Updated pip Proxy"

    echo -e "\033[1;33m\n>> Configuring the docker proxy\n\033[0m"
    Docker_Proxy > /dev/null
    systemctl restart docker > /dev/null 2>&1 || Echo_ERROR "Failed to restart docker service!(Ignore the error if docker is not installed)"
    systemctl daemon-reload > /dev/null 2>&1 || Echo_ERROR "Failed to reload the configuration file of the systemd!(You can ignore the error)"

    Echo_INFOR "Updated docker Proxy"

}

# -debug
Debug_Fun(){

    echo -e "none :>"

}

# -mock
Mock_Test(){

    $1

}

# Main
Main(){

case "$(uname)" in
    *"Darwin"*)
	    echo "Not supported on mac platform"
	    exit 1
        ;;
    *"MINGW64_NT"*)
        echo "Not supported on windows platform"
        exit 1
        ;;
esac

if [[ $UID != 0 ]]; then
    Echo_ERROR "Please run with sudo or root privileged account!"
    exit 1
fi

setenforce 0 > /dev/null 2>&1

printf "\033c"

}

Main

if [ $GitPorxy ==  ] 2>> /tmp/f8x_error.log
then
    Banner
fi

Info

if [ $GitPorxy ==  ] 2>> /tmp/f8x_error.log
then
    Info2
    echo -e "\033[1;36m \n-----Start execution----- \033[0m"
    echo -e "\033[1;33m\n>> Initializing\n\033[0m"
    Base_Dir
    Docker_run_Check
fi

for cmd in $@
do
    case $cmd in
        -b | b)
            Base_Install
            ;;
        -p | p)
            Proxy
            ;;
        -d | d)
            Porxy_Switch
            Base_Check
            Dev_Tools
            ;;
        -k | k)
            Porxy_Switch
            Base_Check
            Py_Check
            kali_Tools
            ;;
        -ka | ka)
            Porxy_Switch
            Base_Check
            Py_Check
            kali_Tools "a"
            ;;
        -kb | kb)
            Porxy_Switch
            Base_Check
            Py_Check
            kali_Tools "b"
            ;;
        -kc | kc)
            Porxy_Switch
            Base_Check
            Py_Check
            kali_Tools "c"
            ;;
        -kd | kd)
            Porxy_Switch
            Base_Check
            Py_Check
            kali_Tools "d"
            ;;
        -ke | ke)
            Porxy_Switch
            Base_Check
            Py_Check
            kali_Tools "e"
            ;;
        -s | s)
            Porxy_Switch
            Base_Check
            Secure
            ;;
        -f | f)
            Porxy_Switch
            Base_Check
            pip3_Check
            Fun_Tools
            ;;
        -h | h)
            printf "\033c"
            Help
            ;;
        -cloud)
            Porxy_Switch
            Base_Check
            cloud
            ;;
        -all)
            all_Install
            ;;
        -lua)
            Porxy_Switch
            Base_Check
            lua_Install
            ;;
        -nn)
            Porxy_Switch
            Base_Check
            nn_Install
            ;;
        -openjdk)
            Porxy_Switch
            Base_Check
            Openjdk_Install
            ;;
        -oraclejdk)
            Porxy_Switch
            Base_Check
            Oraclejdk_Install
            ;;
        -python3)
            Python3_Install
            ;;
        -python2)
            Python2_Install
            ;;
        -pip2-f)
            Porxy_Switch
            Base_Check
            pip2_Install
            ;;
        -perl)
            Porxy_Switch
            Base_Check
            Perl_Install
            ;;
        -ruby)
            Porxy_Switch
            Base_Check
            Ruby_Install
            ;;
        -ruby-f)
            Porxy_Switch
            Base_Check
            Ruby_Install_f
            ;;
        -rust)
            Porxy_Switch
