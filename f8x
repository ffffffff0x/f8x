        pip3 install -r requirements.txt > /dev/null 2>&1 && Echo_INFOR "Installed dependency modules" || ( Echo_ERROR "Failed to install dependency module" && rm -rf $P_Dir/SecretFinder > /dev/null 2>&1 )
