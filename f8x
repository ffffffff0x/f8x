            arl_Install
            ;;
        -awvs14)
            Porxy_Switch
            awvs14_Install
            ;;
        -mobsf)
            Porxy_Switch
            mobsf_Install
            ;;
        -nodejsscan)
            Porxy_Switch
            nodejsscan_Install
            ;;
        -viper)
            Porxy_Switch
            viper_Install
